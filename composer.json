{
    "name": "micro-module/job-queue",
    "type": "library",
    "description": "Job Queue",
    "keywords": ["messaging", "queue", "jobs"],
    "homepage": "https://enqueue.forma-pro.com/",
    "license": "MIT",
    "require": {
        "php": "^7.4 || ^8.0",
